## The base URL for easy api. (This is recommended to use the easy-api on api node of SAP Commerce Cloud environment and configure URL accordingly)
sap.commerce.easy.api.base.url = ${EASY_API_BASE_URL}

## API Key to access Easy API
sap.commerce.easy.api.key = ${EASY_API_KEY}

## The Easy version for your easy extensions
sap.commerce.easy.extension.easy.version = ${EASY_VERSION}

## Base package for your easy extensions
sap.commerce.easy.extension.base.package = ${EASY_EXTENSION_BASE_PACKAGE}

## The easy repository code for your easy extensions in SAP Commerce Cloud environment
sap.commerce.easy.repository.code = ${EASY_REPOSITORY_CODE}

## The easy extension id for the extension
sap.commerce.easy.extension.id = ${EASY_EXTENSION_ID}

